{
  "name": "node-api-rest",
  "version": "1.0.0",
  "description": "First steps to creation API REST with node",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeders:generate": "sequelize-cli seed:generate --name",
    "seeders:run-all": "sequelize-cli db:seed:all",
    "seeders:run-delete": "sequelize-cli db:seed:undo",
    "seeders:run-name": "sequelize-cli db:seed:undo --seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noriega402/Node-API-REST.git"
  },
  "keywords": [
    "NodeJS",
    "postgreSQL",
    "sequelize",
    "express"
  ],
  "author": "Daniel Noriega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Noriega402/Node-API-REST/issues"
  },
  "homepage": "https://github.com/Noriega402/Node-API-REST#readme",
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "sequelize-cli": "^6.6.0",
    "mysql2": "^3.2.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "faker": "5.5.3",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.3",
    "yup": "^0.32.11"
  }
}
